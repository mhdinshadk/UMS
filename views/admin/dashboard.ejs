                  </div>
              </nav>
              <div class="pcoded-content">
                  <div class="pcoded-inner-content">

                      <div class="main-body">
                          <div class="page-wrapper">
                              

                              <div class="page-body">
                                  <div class="row">
                                      <div class="col-sm-12">
                                          <div class="card">
                                              <div class="card-header">
                                                  <h5>users</h5>
                                                
                                                  
                                              </div>
                                              <div class="card-block">
                                                <table class="table text-start align-middle table-bordered table-hover mb-0">
                                                  <thead>
                                                      <tr >
                                                          <th scope="col">Name</th>
                                                          <th scope="col">@Email</th>
                                                          <th scope="col">Mobile</th>
                                                          <th scope="col">Dp</th>
                                                          <th scope="col">Status</th>
                                                          <th scope="col">Actions</th>
                                                      </tr>
                                                  </thead>
                                                  <tbody>
                                                      <% 
                                if(users.length > 0){
                                 for(let i=0;i<users.length;i++){
                                   %>
                                                      <tr>
                                                          <td><%= users[i].name %></td>
                                      <td><%= users[i].email %></td>
                                      <td><%= users[i].mobile %></td>
                                      <td> 
                                        
                                        <div class="position-relative">
                                            <img class="rounded-circle" src="/userImages/<%= users[i].image %>" alt="<%= users[i].image %>" style="width: 100px; height: 100px;">
                                
                                        
                                        <!-- <img src="/userImages/<%= users[i].image %>" alt="<%= users[i].image %>" width="100px" height="100px"></td> -->
                                      <td><%
                                          if(users[i].is_varified == 0){
                                            %>
                                             <p>Not Verified <i class="fa fa-exclamation" style="color: rgb(255, 3, 3);"></i></p>
                                            <%
                                          } 
                                          else{
                                            %>
                                            <p>Verified <i class="fa fa-check" style="color: rgb(106, 255, 0);"></i></p> 
                                
                                            <%
                                          }
                                           %></td>
                                                          <td>
                                                              <a  class="btn  btn-success" href="/admin/edit-user?id=<%= users[i]._id %>">Edit <i class="fa fa-edit"></i></a>
                                                              <a  class="btn  btn-primary" href="/admin/delete-user?id=<%= users[i]._id %>#">Delete <i class="fa fa-trash"></i></a>
                                                          </td>
                                                      </tr>
                                                      <%
                                 }
                                }
                                else{
                                %>
                                <tr>
                                  <td colspan="5">Users Not Found</td>
                                </tr>
                                <%
                                }
                                %>
                                                      
                                                      
                                                      
                                                  </tbody>
                                              </table>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                      </div>
                      
                  </div>
              </div>
          </div>
      </div>
  </div>
</div>
